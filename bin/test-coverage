#!/usr/bin/env bash
set -euo pipefail
exec > >(tee log/test-coverage.log) 2>&1 

echo "> test-coverage"

echo "> test-coverage > cargo llvm-cov nextest"
RUSTFLAGS='-D warnings' cargo llvm-cov nextest \
  --ignore-filename-regex 'run_dot\.rs' \
  --fail-under-functions 64 \
  --fail-under-lines 71 \
  --fail-under-regions 73

