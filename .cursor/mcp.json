{
	"mcpServers": {
		"serena": {
			"command": "uvx",
			"args": [
				"--from",
				"git+https://github.com/oraios/serena",
				"serena",
				"start-mcp-server",
				"--context",
				"ide"
			],
			"disabled": false
		},
		"context7": {
			"command": "npx",
			"args": ["-y", "@upstash/context7-mcp@latest"],
			"disabled": false
		},
		"ferroscope": {
			"command": "ferroscope",
			"args": [],
			"env": {},
			"description": "Rust debugging via LLDB/GDB"
		},
		"mcp-debugger": {
			"command": "node",
			"args": [
				"node_modules/.bin/mcp-debugger",
				"stdio",
				"--log-level",
				"debug",
				"--log-file",
				"./tmp/debug-mcp-server.log"
			],
			"disabled": false,
			"autoApprove": ["create_debug_session", "set_breakpoint", "get_variables"]
		}
	}
}
